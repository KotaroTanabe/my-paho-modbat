digraph model {
  orientation = landscape;
  graph [ rankdir = "TB", sep="0.5", nodesep="0.2" ];
  node [ fontname = "Helvetica", fontsize="12.0", margin="0.07" ];
  edge [ fontname = "Helvetica", fontsize="12.0", margin="0.05" ];
  "" [ shape = "point", height="0.1" ];
  "" -> "init"
  "init"	-> "run" [ label = " start " ];
}
